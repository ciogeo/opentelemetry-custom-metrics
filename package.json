{
  "name": "opentelemetry-custom-metrics",
  "version": "1.0.31",
  "description": "Allows you to add custom OpenTelemetry metrics in your NestJS application",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf build/*",
    "build": "tsc --project tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ciogeo/opentelemetry-custom-metrics.git"
  },
  "keywords": [
    "opentelemetry",
    "nestjs",
    "metrics",
    "stats",
    "prometheus",
    "access",
    "time"
  ],
  "author": "George Ciochinaru",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/ciogeo/opentelemetry-custom-metrics/issues"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.9",
    "@opentelemetry/api": "^1.3.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.2.1",
    "@types/jest": "28.1.4",
    "gts": "3.1.0",
    "rimraf": "3.0.2",
    "typescript": "4.3.5",
    "jest": "28.1.2",
    "ts-jest": "^28.0.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ciogeo/opentelemetry-custom-metrics#readme",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "./src/**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
